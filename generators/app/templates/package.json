{
  "name": "<%= service_name %>",
  "version": "0.2.0",
  "private": true,
  "description": "<%= description %>",
  "keywords": [],
  "author": "<%= author %>",
  "license": "MIT",
  "main": "app.js",
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.0",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "morgan": "~1.7.0"
  },
  "devDependencies": {
    "nsp": "^2.6.3",
    "eslint": "^3.18.0",
    "eslint-config-xo-space": "^0.16.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "camelcase": "off"
    }
  },
  "scripts": {
    "start": "node app.js",
    "prepublish": "nsp check",
    "pretest": "eslint . --ignore-path .gitignore --fix",
    "test": "jest"
  },
  "directories": {
    "test": "test/"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= github_user %>/<%= service_name %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= github_user %>/<%= service_name %>/issues"
  },
  "homepage": "https://github.com/<%= github_user %>/<%= service_name %>#readme"
}
